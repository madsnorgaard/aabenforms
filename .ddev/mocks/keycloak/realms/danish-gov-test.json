  "clients": [
    {
      "clientId": "aabenforms-backend",
      "name": "Ã…benForms Backend (Drupal)",
      "description": "Ã…benForms Drupal backend OIDC client",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "clientAuthenticatorType": "client-secret",
      "secret": "aabenforms-backend-secret-change-in-production",
      "redirectUris": [
        "http://localhost:3000/*",
        "http://aabenforms.ddev.site/*",
        "https://aabenforms.ddev.site/*"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "http://aabenforms.ddev.site",
